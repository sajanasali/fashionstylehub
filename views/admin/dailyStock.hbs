{{>adminheader}}
{{#if stock}}

<div class="container mt-3">

 <a class="btn btn-secondary ml-3 mt-3"  href="/admin/dailystock/download">Xls</a>
<button class="btn btn-success ml-3 mt-3" onclick="printPage()">Print</button>


    <table class="table p-5" >
        <thead class="thead-dark">

            <tr>
                
                <th scope="col">PRODUCT NAME</th>

                <th scope="col">PRODUCT QUANTITY</th>
                {{!-- <th scope="col">DISCOUNT</th> --}}
                

            </tr>

        </thead>

        <tbody>

        {{#each stock}}
            <tr>

               
                <td>{{this.name}}</td>
                <td>{{this.quantity}}</td>
                {{!-- <td>{{this.discount}}</td> --}}
                


            </tr>
                  {{/each}}



        </tbody>


    </table>
</div>
{{else}}
<div class="container p-5">
    <h2>Nothing Here</h2>
</div>
{{/if}}

</div>
<script>
    function printPage() {
  window.print();
}

</script>


